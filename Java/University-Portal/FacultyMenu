private JFrame Logout;
    private void btnLogOutActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        Logout = new JFrame("LogOut");
        if(JOptionPane.showConfirmDialog(Logout, "Comfirm if you want to Log Out","MySQL Connector",
       JOptionPane.YES_NO_CANCEL_OPTION)==JOptionPane.YES_OPTION)
            
        {
            // Close the current form
        this.dispose();
        
        // Open the login form
        LoginForm loginForm = new LoginForm();
        loginForm.setVisible(true);
        }
    }                                         

    private void btnShowStuActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        ShowStudents showStudentsPage = new ShowStudents(); // Instantiate StudentProfile page
        showStudentsPage.setVisible(true); // Display the StudentProfile page
    }                                          

    private void btnMarksActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        StudentsMarks studentsMarksPage = new StudentsMarks(); // Instantiate StudentProfile page
        studentsMarksPage.setVisible(true); // Display the StudentProfile page
        
    }                                        

