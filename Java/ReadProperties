package readproperties;

/**
 *
 * @author Revy
 */
import java.util.*;
import java.io.*;
public class ReadProperties {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Properties p = new Properties(); // I have put the data in the file
        p.setProperty("k1", "v1");
        p.setProperty("k2", "v2");
        p.list(System.out);  //Whats in the object
        try{
            //create or replace file
            FileOutputStream out = new FileOutputStream("myProps1.props");
            p.store(out, "test-comment");  //Adds header comment
            out.close();
        } catch (IOException e) {
            System.out.println("exc 1"); //Prints whats inside the file
        }
    }
    
}

